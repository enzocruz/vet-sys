@model IEnumerable<Vet.Web.Models.Doctor>
@{
   // ViewData["Title"] = "Index";
   // Layout = "/Views/Shared/_Layout.cshtml";
}
<h3>Doctors</h3>
<div class="row">
    <div class="col-md-12">
         <a class="btn btn-outline-success"  asp-controller="Doctor" asp-action="Create">Add Doctors</a>
    </div>
</div>
<br/>
<div class="row">
    <div class="col-md-12">
        <table class="table table-striped">
           <tr>
                <th>Name</th>
                <th>Address</th>
                <th>License</th>
                <th>Contact Number</th>
                
                
           </tr>
           <tbody>
                 @foreach (var items in Model)
                {
                    <tr>
                        <td>@items.Name</td>
                        <td>@items.Address</td>
                        <td>@items.License</td>
                        <td>@items.Number</td>
                        
                    </tr>
                }
           </tbody>
          
        </table>
    </div>
</div>


